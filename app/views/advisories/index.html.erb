<p id="notice"><%= notice %></p>

<h1>Advisories</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @advisories.each do |advisory| %>
      <tr>
        <td><%= link_to 'Show', advisory %></td>
        <td><%= link_to 'Edit', edit_advisory_path(advisory) %></td>
        <td><%= link_to 'Destroy', advisory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Advisory', new_advisory_path %>
